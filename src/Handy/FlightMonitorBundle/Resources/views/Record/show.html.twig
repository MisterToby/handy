{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Record</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Reclowestoutboundprice</th>
                <td>{{ entity.recLowestOutboundPrice }}</td>
            </tr>
            <tr>
                <th>Reclowestinboundprice</th>
                <td>{{ entity.recLowestInboundPrice }}</td>
            </tr>
            <tr>
                <th>Recdate</th>
                <td>{{ entity.recDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('record') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('record_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
